{
  "name": "server-lamquangdai",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec babel-node server.js",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "db:create": "sequelize-cli db:create",
    "migrate:create": "sequelize-cli migration:generate",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:down": "sequelize-cli db:migrate:undo",
    "seed:create": "sequelize-cli seed:generate",
    "seed:up": "sequelize-cli db:seed:all",
    "seed:down": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.5",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1"
  }
}
